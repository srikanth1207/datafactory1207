{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sDataFactory1207"
		},
		"logdata_metrics_sdatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'logdata_metrics_sdatabase'"
		},
		"sdatalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'sdatalake'"
		},
		"sstorage1207_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'sstorage1207'"
		},
		"sstorage1207a_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sstorage1207a'"
		},
		"synapse_logdataparquet_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'synapse_logdataparquet'"
		},
		"sdatalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sdatalake1207.dfs.core.windows.net/"
		},
		"sstorage1207_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sstorage1207.dfs.core.windows.net/"
		},
		"blobtrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/9ead8410-20b6-4ea4-a404-490f02a7f2a4/resourceGroups/sresource1207/providers/Microsoft.Storage/storageAccounts/sstorage1207"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sDataFactory1207_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"logdata_metrics_sdatabase_connectionString": {
						"value": "[parameters('logdata_metrics_sdatabase_connectionString')]"
					},
					"sdatalake_accountKey": {
						"value": "[parameters('sdatalake_accountKey')]"
					},
					"sstorage1207_accountKey": {
						"value": "[parameters('sstorage1207_accountKey')]"
					},
					"sstorage1207a_connectionString": {
						"value": "[parameters('sstorage1207a_connectionString')]"
					},
					"synapse_logdataparquet_connectionString": {
						"value": "[parameters('synapse_logdataparquet_connectionString')]"
					},
					"sdatalake_properties_typeProperties_url": {
						"value": "[parameters('sdatalake_properties_typeProperties_url')]"
					},
					"sstorage1207_properties_typeProperties_url": {
						"value": "[parameters('sstorage1207_properties_typeProperties_url')]"
					},
					"blobtrigger_properties_typeProperties_scope": {
						"value": "[parameters('blobtrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}